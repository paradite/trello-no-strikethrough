{
  "manifest_version": 2,

  "name": "Trello No Strikethrough",
  "description": "Removes the strikethrough for completed tasks on Trello",
  "icons": { "128": "icon.png" },
  "version": "1.0",
  "background": {
    "page": "popup.html"
  },
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html",
    "default_title": "Trello No Strikethrough"
  },
  "content_scripts": [
    {
      "matches": ["*://trello.com/*"],
      "css": ["mystyles.css"]
    }
  ],
  "permissions": [
    "activeTab",
    "https://trello.com/*"
  ]
}
